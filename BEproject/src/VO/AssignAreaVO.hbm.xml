<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="VO.AssignAreaVO" table="AssignArea">
<id name="assignAreaId" type="int" column="Id">
<generator class="increment"></generator>
</id>
<many-to-one name="staffVO" class="VO.StaffVO" column="StaffID"></many-to-one>

<many-to-one name="areaVO" class="VO.AreaVO" column="AreaID"></many-to-one>



</class>
</hibernate-mapping>